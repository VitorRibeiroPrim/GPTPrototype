{
  "AzureInsightsTelemetryOptions": {
    "DeveloperMode": true
  },
  "HostConfiguration": {
    "ServiceCatalogServiceBaseUri": "https://st-scs.lithium.primaverabss.com"
  },
  "HttpTelemetryOptions": {
    "Rules": {
      "*": {
        "RequestBody": "Enabled",
        "ResponseBody": "Disabled"
      }
    }
  },
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  }
}
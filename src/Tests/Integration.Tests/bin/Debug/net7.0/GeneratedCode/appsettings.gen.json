{
  "AzureInsightsTelemetryOptions": {
    "DefaultProperties": {
      "Lithium-Service": "ChatGPT"
    },
    "DeveloperMode": false,
    "EnableAdaptiveSampling": false,
    "Enabled": true,
    "ExceptionTracking": {
      "DetectHttpRequestsAborted": true
    },
    "IgnoreErrors": true,
    "RequestTracking": {
      "IgnoreAvailabilityTestsRequests": true,
      "IgnoreConflictPaths": [
        "/api/"
      ],
      "IgnoreNotFoundPaths": [
        "/api/",
        "/admin/functions"
      ]
    }
  },
  "HostConfiguration": {
    "ServiceCatalogServiceBaseUri": "https://scs.lithium.primaverabss.com"
  },
  "HttpTelemetryOptions": {
    "Enabled": "Enabled",
    "LogLevel": "Information",
    "Rules": {
      "*": {
        "Enabled": "Enabled",
        "RequestBody": "Disabled",
        "ResponseBody": "Disabled"
      },
      "/.monitoring/": {
        "Enabled": "Enabled",
        "RequestBody": "Disabled",
        "ResponseBody": "FailuresOnly"
      }
    }
  },
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}